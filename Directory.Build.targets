<Project>
    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <OutputPath Condition=" '$(CompatibilityFor)' == '' ">$(_OutputDirectory)</OutputPath>
        <OutputPath Condition=" '$(CompatibilityFor)' != '' ">$(_CompatibilityOutputDirectory)$(CompatibilityFor)\$(RimWorldVersion)\Assemblies</OutputPath>
    </PropertyGroup>
    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
        <OutputPath Condition=" '$(CompatibilityFor)' == '' ">$(_OutputDirectory)</OutputPath>
        <OutputPath Condition=" '$(CompatibilityFor)' != '' ">$(_CompatibilityOutputDirectory)$(CompatibilityFor)\$(RimWorldVersion)\Assemblies</OutputPath>
    </PropertyGroup>
</Project>
